
h1 {
		margin-left: 20rpx;
}
.custom-list {
		list-style: none;
}
.card-icon {
		width: 100%;
		display: flex;
		flex-direction: column;
		align-items: center;
		transform: translateY(-60rpx);
}
.card-icon .container {
		display: flex;

		justify-content: center;
		align-items: center;
		width: 80rpx;
		height: 80rpx;
		border: 5rpx solid #FCA464;
		border-radius: 100%;
}
.card-icon img {
		width: 50rpx;
		height: 50rpx;
		object-fit: fill;
}
.content {
		margin: 30rpx;
		font-weight: 550;
		line-height: 50rpx;
		font-size: 28rpx;
}
.cardicon2 uni-icons {}
.custom-list {
		display: flex;
}
.custom-list img {
		width: 50rpx;
		height: 50rpx;
}
.custom-list view {
		line-height: 50rpx;
		list-style-type: none;
		vertical-align: middle;
		font-size: 40rpx;
}
.icon {
		width: 100rpx;
		left: 20rpx;
		display: flex;
		flex-direction: column;
		align-items: center;
		height: 100%;
		margin-right: 15rpx;
}
.icon img {
		width: 100rpx;
		height: 120rpx;
		object-fit: fill;
}
